{
  "manifest_version": 2,
  "name": "vvz-coursereview",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://n.ethz.ch/~lteufelbe/*",
    "https://rubberducky.vsos.ethz.ch/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.vvz.ethz.ch/*",
        "https://www.vorlesungen.ethz.ch/*"
      ],
      "js": [
        "coursereview-links.js",
        "rating.js",
        "timetable.js",
        "filter.js",
        "autofill.js",
        "page-editor.js"
      ]
    }
  ]
}